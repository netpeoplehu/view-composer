{
  "version": "1.1.0",
  "name": "netpeoplehu/view-composer",
  "description": "View composer package for Laravel >= 5.4",
  "homepage": "https://github.com/netpeoplehu/view-composer",
  "keywords": [
    "laravel",
    "views",
    "view",
    "view-composer",
    "composer"
  ],
  "authors": [
    {
      "name": "Egon Richárd Tőrös",
      "email": "egon@netpeople.hu",
      "homepage": "http://netpeople.hu",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/netpeoplehu/view-composer/releases",
    "issues": "https://github.com/netpeoplehu/view-composer/issues"
  },
  "require": {
    "illuminate/console": ">= 5.4",
    "illuminate/support": ">= 5.4",
    "illuminate/filesystem": ">= 5.4"
  },
  "autoload": {
    "psr-4": {
      "Netpeople\\ViewComposer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Netpeople\\ViewComposer\\Providers\\ViewComposerServiceProvider"
      ]
    }
  },
  "license": "MIT"
}
